{
  "name": "github-stats-generator",
  "version": "1.0.0",
  "description": "Beautiful, customizable GitHub statistics cards for your README",
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/github-stats-generator.git"
  },
  "keywords": [
    "github",
    "stats",
    "readme",
    "badges",
    "profile",
    "open-source",
    "nextjs",
    "svg"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@octokit/graphql": "^7.1.0",
    "@primer/octicons": "^19.21.1",
    "@primer/octicons-react": "^19.21.1",
    "@vercel/kv": "^3.0.0",
    "lucide-react": "^0.263.1",
    "next": "^14.2.0",
    "node-cache": "^5.1.2",
    "react": "^19.2.3",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/primer__octicons": "^19.11.1",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  }
}